{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./dist/",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "module": "ESNext",
    "target": "ESNext",
    "strict": false,
    "noImplicitAny": false,
    "sourceMap": true,
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}